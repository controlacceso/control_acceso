extends layout

block content
  h1= title
  p #{title} 
   table
    thead
    tbody
    - for (var i = 0; i < nombre.length; ++i) {
        tr
          td
           img(alt='foto', src='data:img/png;base64,#{foto[i]}', width=100, height=100)
           p= nombre[i]+" "+apellidos[i]
    - }
  a(href='/') Volver
  div#horario
   p Horario profesor