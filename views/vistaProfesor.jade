extends layout

block content
  h1= title
  p #{title} 
   table
    thead
    tbody
      - each item in info
        tr
          td
           img(alt='foto', src='data:img/png;base64,#{item.foto}', width=100, height=100)
           p= item.nombre+" "+item.apellidos
  a(href='/') Volver
  div#horario
   p Horario profesor